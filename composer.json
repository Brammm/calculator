{
    "name": "brammm/calculator",
    "description": "Calculates infix notation",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Van der Sype",
            "email": "bram.vandersype@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
